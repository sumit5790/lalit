pipeline{
         agent none
		 stages{
		 
		 
		 stage('Front-end') {
            agent {
                        dockerfile {
                                   filename 'Dockerfile'
                                   dir 'build'
                                   label 'docker_file'
                                   registryUrl 'https://172.31.30.63:5000'
                                   registryCredentialsId 'credentials-id'
    }
	}	
         }	
		    stage('build'){
		             steps{
			      sh 'docker --version'	
			       }
				  }
			
			
			
			stage('skiptest'){
			  
			         steps{     
					    sh 'mvn -B -DskipTests clean package'
					 }
			}
			
			
		} 
		 
		 }

